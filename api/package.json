{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@redwoodjs/api": "0.0.1-alpha.16",
    "@prisma/photon": "2.0.0-preview019"
  },
  "devDependencies": {
    "@redwoodjs/dev-server": "0.0.1-alpha.16",
    "prisma2": "2.0.0-preview019"
  },
  "scripts": {
    "dev": "yarn dev-server",
    "db:up": "yarn prisma2 lift up && yarn prisma2 generate",
    "db:save": "yarn prisma2 lift save",
    "db:seed": "node prisma/seeds",
    "build": "NODE_ENV=production yarn db:up && NODE_ENV=production babel src --out-dir dist",
    "postinstall": "yarn db:up"
  }
}
